<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.Users" table="users" schema="SCOTT">
		<id name="usersid" type="java.lang.String">
			<column name="usersid" length="100" />
			<generator class="uuid.hex"></generator>
		</id>
		<property name="username" type="java.lang.String">
			<column name="username" length="50" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="50" />
		</property>
		<property name="realname" type="java.lang.String">
			<column name="realname" length="50" />
		</property>
		<property name="sex" type="java.lang.String">
			<column name="sex" length="50" />
		</property>
		<property name="birthday" type="java.lang.String">
			<column name="birthday" length="50" />
		</property>
		<property name="idcard" type="java.lang.String">
			<column name="idcard" length="50" />
		</property>
		<property name="contact" type="java.lang.String">
			<column name="contact" length="50" />
		</property>
		<property name="image" type="java.lang.String">
			<column name="image" length="50" />
		</property>
		<property name="regdate" type="java.lang.String">
			<column name="regdate" length="50" />
		</property>
	</class>
</hibernate-mapping>
