<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.Rebbs" table="rebbs" schema="SCOTT">
		<id name="rebbsid" type="java.lang.String">
			<column name="rebbsid" length="100" />
			<generator class="uuid.hex"></generator>
		</id>
		<property name="usersid" type="java.lang.String">
			<column name="usersid" length="50" />
		</property>
		<property name="bbsid" type="java.lang.String">
			<column name="bbsid" length="50" />
		</property>
		<property name="contents" type="java.lang.String">
			<column name="contents" length="50" />
		</property>
		<property name="addtime" type="java.lang.String">
			<column name="addtime" length="50" />
		</property>
		<many-to-one name="users" column="usersid" class="com.entity.Users"
			fetch="join" insert="false" update="false" lazy="false" />
	</class>
</hibernate-mapping>
